
const s = "pwwkew"
let windowLeft = 0
let maxLength = 0
const dataSet = new Set()

for (let windowRight = 0; windowRight < s.length; windowRight++) {

    while (dataSet.has(s[windowRight])) {
        dataSet.delete(s[windowLeft])
        windowLeft++
    }

    dataSet.add(s[windowRight])
    maxLength = Math.max(maxLength, windowRight - windowLeft + 1)
}

console.log(maxLength)  // Output â†’ 3
